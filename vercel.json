{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel_main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel_main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  },
  "functions": {
    "api/vercel_main.py": {
      "maxDuration": 30
    }
  }
}
